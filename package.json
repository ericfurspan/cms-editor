{
  "name": "cms",
  "version": "0.3.0-beta",
  "private": true,
  "scripts": {
    "build:dev": "webpack --mode=development --config ./webpack.config.js --debug",
    "start:dev": "webpack-dev-server --config ./webpack.config.js --mode=development --inline --progress",
    "run:dev": "npm run build:dev && npm run start:dev",
    "build": "NODE_ENV=production webpack --mode production --progress --colors -p --config ./webpack.config.js",
    "copy": "cp ./express.js ./build/",
    "build:copy": "npm run build && npm run copy",
    "start": "node --require 'dotenv/config' ./build/express.js",
    "go": "npm run build && npm run copy && npm run start",
    "test": "jest",
    "lint": "eslint --ext .jsx --ext .js ./src",
    "prettier": "prettier --write .",
    "build:prod:stats": "NODE_ENV=production webpack --mode production --profile --config ./webpack.config.js --json > stats.json",
    "inspectpack:duplicates": "inspectpack -s stats.json -a duplicates",
    "inspectpack:versions": "inspectpack -s stats.json -a versions"
  },
  "dependencies": {
    "@apollo/client": "^3.1.3",
    "@apollo/link-error": "^2.0.0-beta.3",
    "@babel/runtime": "^7.9.6",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@sentry/react": "^5.22.3",
    "@sentry/tracing": "^5.22.3",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^10.0.4",
    "@testing-library/user-event": "^10.4.0",
    "apollo-upload-client": "^13.0.0",
    "babel-jest": "^26.0.1",
    "bootstrap": "^4.5.0",
    "clean-webpack-plugin": "^3.0.0",
    "colors": "^1.4.0",
    "copy-webpack-plugin": "^6.0.1",
    "css-loader": "3.5.3",
    "dotenv-webpack": "^1.8.0",
    "eslint": "^7.1.0",
    "express": "^4.17.1",
    "favicons-webpack-plugin": "^6.0.0",
    "file-loader": "6.0.0",
    "formik": "^2.1.4",
    "framer-motion": "^1.11.1",
    "graphql": "^15.0.0",
    "history": "^4.10.1",
    "html-webpack-plugin": "4.4.1",
    "jest": "26.0.1",
    "jest-resolve": "26.0.1",
    "lodash": "^4.17.20",
    "mini-css-extract-plugin": "0.9.0",
    "omit-deep-lodash": "^1.1.4",
    "react": "^16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-content-loader": "^5.1.0",
    "react-dom": "^16.13.1",
    "react-json-view": "^1.19.1",
    "react-router-dom": "^5.2.0",
    "sass-loader": "8.0.2",
    "style-loader": "1.2.1",
    "styled-components": "^5.1.1",
    "sweetalert2": "^9.17.1",
    "sweetalert2-react-content": "^3.0.3",
    "url-loader": "4.1.0",
    "webpack": "4.43.0",
    "webpack-cli": "^3.3.11",
    "workbox-webpack-plugin": "^5.1.3",
    "yup": "^0.29.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-destructuring": "^7.9.5",
    "@babel/plugin-transform-regenerator": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
